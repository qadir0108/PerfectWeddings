<script>


    function CloseAgreement() {
        var ajaxModal = $('#config-modal-list');

        $.ajax({
            // contentType: 'application/json; charset=utf-8',
            url: '/Account/CloseAgreement',
            type: "GET",
            traditional: true,
            // dataType: 'json',
            success: function (response) {
                if (response != null && response.key == false) {
                    showToaster({
                        msg: response.message, priority: "error"
                    });

                } else {
                    showToaster({
                        msg: "Agreement accepted by you", priority: "success"
                    });
                    
                
                    window.location.href = "/Account/Login";
                    ajaxModal.modal('hide');

                }
            },
            error: function (xhr, desc, err) {
                console.log(xhr);
                alert(err);
                console.log("Desc: " + desc + "\nErr:" + err);
            }
        });
    }
